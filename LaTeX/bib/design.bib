	shorttitle = {Book {Club} {Model} for {Engaging} with {Data} {Science} and {Ethics}},
	title = {Book {Club} {Model} for {Engaging} with {Data} {Science} and {Ethics}: {Using} {Weapons} of {Math} {Destruction}},
	url = {https://doi.org/10.1145/3626252.3630792},
