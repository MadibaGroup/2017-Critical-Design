@inproceedings{lc_human_2023,
	abstract = {The cave painting was a creative space we no longer understand. What technologies of today can we use to understand our own concerns of a climate future analogous to the way pre-historic humans drew their conceptions onto the cave? HUMAN ENOUGH uses machine learning-generated visions of climate futures (Stable Diffusion, Midjourney, etc) and climate adaptions / potential solutions (ChatGPT) in a creative space (Gather) to reconstruct a modern analog of the age-old cave painting. It then constructs the machine visions into physical installed objects using recycled, organic, found materials from site-specific builds for physical exhibition. The outcome is a collective imagining of our climate future and our adaptions to it from a technological and material perspective.},
	address = {New York, NY, USA},
